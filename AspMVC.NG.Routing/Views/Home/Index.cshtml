<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>UTA Water Polo</title>
    
    
    
    
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>

    
    <link href="~/Assets/css/bootstrap.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="../Assets/css/custom.min.css">
    <link rel="stylesheet" href="../Assets/css/Site.css">
    <link rel="stylesheet" href="~/Assets/css/images3D.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <script src="//code.angularjs.org/snapshot/angular.min.js"></script>
    <script src="//code.angularjs.org/snapshot/angular-route.js"></script>

    <script src="//code.angularjs.org/snapshot/angular-animate.min.js"></script>
    <script src="//code.angularjs.org/snapshot/angular-messages.min.js"></script>
    <script src="//code.angularjs.org/snapshot/angular-aria.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-material/1.1.0/angular-material.min.js"></script>
    <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/angular_material/1.1.0/angular-material.min.css">
    
   
        
    <style type="text/css">
        /* required style*/
        .none {
            display: none;
        }

        /* optional styles */
        table tr th, table tr td {
            font-size: 1.2rem;
        }

        .row {
            margin: 20px 20px 20px 20px;
            width: 100%;
        }

        .glyphicon {
            font-size: 20px;
        }

        .glyphicon-plus {
            float: right;
        }

        a.glyphicon {
            text-decoration: none;
            cursor: pointer;
        }

        .glyphicon-trash {
            margin-left: 10px;
        }

        .alert {
            width: 50%;
            border-radius: 0;
            margin-top: 10px;
            margin-left: 10px;
        }
        .enter_login {
            background: #122737;
            background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #081d2f), color-stop(1, #707d87));
            float: left;
            border-right: 1px solid #4c6d8e;
            border-bottom: 1px solid #193a54;
            -webkit-transform: skewX(-15deg);
            -moz-transform: skewX(-15deg);
            -ms-transform: skewX(-15deg);
            -o-transform: skewX(-15deg);
            transform: skewX(-15deg);
        }
        #navbar-main ul li.enter_login a:hover,
        #navbar-main ul li.enter_login a:focus {
            background: #122737;
        }


    </style>
    <script type="text/javascript">
        angular.element(document.getElementsByTagName('head')).append(angular.element('<base href="' + window.location.pathname + '" />'));
    </script>
</head>
<body ng-app="UTAWaterPoloApp">
<div class="navbar navbar-default navbar-static-top"  style="top: 20px">
    <div class="container">
        <div class="navbar-header">
            <a href="/" class="logo"><img style="width:130px; height: 110px;" src="~/Assets/Images/wplogo.png"></a>
            &emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;
            <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div class="navbar-collapse collapse" id="navbar-main">
            <ul class="nav navbar-nav">

                <li>
                    <a href="/">HOME</a>
                </li>
                <li>
                    <a href="About">ABOUT</a>
                </li>
                <li>
                    <a href="Events">EVENTS</a>
                </li>
                <li>
                    <a href="Gallery">GALLERY</a>
                </li>

            </ul>

            <ul class="nav navbar-nav navbar-right">
                <li class="enter_login" ng-show="admin_status"><a href="register">ADD NEW MEMBER</a></li>
                <li class="enter_login" ng-show="admin_status"><a href="Admin">ADMIN</a></li>
                <li class="enter_login" ng-show="login_status"><a href="logout">LOGOUT</a></li>
                <li class="enter_login" ng-show="!login_status"><a href="login">LOGIN</a></li>
            </ul>
        </div>
    </div>
</div>
<div ng-view></div>

@Scripts.Render("~/bundles/UTAWaterPoloApp")
</body>
</html>

